<div class = "container">
<h1>Lists</h1>
<p> <%= link_to "Add a new list", new_list_path(@list), class: "btn btn-primary" %></p>
  <ul class = "list-group">
    <% @lists.each do |list| %>
      <li class = "list-group-item"><%= link_to list.name,list_path(list) %></li>
    <% end %>
  </ul>
</div>
